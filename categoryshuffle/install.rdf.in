<?xml version="1.0" encoding="UTF-8"?>

#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <!-- UUID for extension, filled in during build -->
    <em:id>@EXTENSION_UUID@</em:id>
    <!-- extensions are type 2 -->
    <em:type>2</em:type>
    <em:name>Category Shuffle</em:name>
    <em:version>0.1.0</em:version>
    <em:creator>repeatingbeats</em:creator>
    <em:description>Shuffle by metadata categories</em:description>
    <em:homepageURL>http://www.repeatingbeats.com</em:homepageURL>
    <em:aboutURL>chrome://categoryshuffle/content/about.xul</em:aboutURL> 
     <!-- extension architecture (OS/CPU) filled in during build -->
    <em:targetPlatform>@EXTENSION_ARCH@</em:targetPlatform>   
    <!-- target application is Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>1.2.0</em:minVersion>
        <em:maxVersion>1.5.0a</em:maxVersion>
      </Description>
    </em:targetApplication> 
  </Description>
</RDF>
